#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nicolasbellina/Desktop/apps/node_modules/.pnpm/tailwind-config-viewer@2.0.4_tailwindcss@3.4.17_ts-node@10.9.2_@types+node@22.13.9_typescript@5.8.2__/node_modules/tailwind-config-viewer/cli/node_modules:/Users/nicolasbellina/Desktop/apps/node_modules/.pnpm/tailwind-config-viewer@2.0.4_tailwindcss@3.4.17_ts-node@10.9.2_@types+node@22.13.9_typescript@5.8.2__/node_modules/tailwind-config-viewer/node_modules:/Users/nicolasbellina/Desktop/apps/node_modules/.pnpm/tailwind-config-viewer@2.0.4_tailwindcss@3.4.17_ts-node@10.9.2_@types+node@22.13.9_typescript@5.8.2__/node_modules:/Users/nicolasbellina/Desktop/apps/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/nicolasbellina/Desktop/apps/node_modules/.pnpm/tailwind-config-viewer@2.0.4_tailwindcss@3.4.17_ts-node@10.9.2_@types+node@22.13.9_typescript@5.8.2__/node_modules/tailwind-config-viewer/cli/node_modules:/Users/nicolasbellina/Desktop/apps/node_modules/.pnpm/tailwind-config-viewer@2.0.4_tailwindcss@3.4.17_ts-node@10.9.2_@types+node@22.13.9_typescript@5.8.2__/node_modules/tailwind-config-viewer/node_modules:/Users/nicolasbellina/Desktop/apps/node_modules/.pnpm/tailwind-config-viewer@2.0.4_tailwindcss@3.4.17_ts-node@10.9.2_@types+node@22.13.9_typescript@5.8.2__/node_modules:/Users/nicolasbellina/Desktop/apps/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli/index.js" "$@"
else
  exec node  "$basedir/../../cli/index.js" "$@"
fi
